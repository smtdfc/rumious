{
  "name": "@rumious/compiler",
  "version": "1.1.0",
  "description": "A compiler for the Rumious framework",
  "license": "MIT",
  "author": "smtdfc <me.smtdfc@gmail.com>",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/",
    "lib/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build:prod": "cross-env NODE_ENV=production rollup -c",
    "build:dev": "cross-env  NODE_ENV=development rollup -c",
    "clean": "shx rm -rf dist",
    "prepublishOnly": "shx cp  ../../LICENSE . && npm run clean && npm run build:prod"
  },
  "keywords": [
    "compiler",
    "jsx",
    "babel",
    "framework",
    "rumious",
    "reactive",
    "template",
    "transform"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/smtdfc/rumious"
  },
  "bugs": {
    "url": "https://github.com/smtdfc/rumious/issues"
  },
  "homepage": "https://github.com/smtdfc/rumious/tree/main/packages/compiler#readme",
  "engines": {
    "node": ">=18"
  },
  "sideEffects": false,
  "dependencies": {
    "@babel/generator": "^7.27.3",
    "@babel/parser": "^7.27.4",
    "@babel/traverse": "^7.27.4",
    "@babel/types": "^7.27.3"
  },
  "peerDependencies": {
    "@rumious/core": "workspace:*"
  },
  "devDependencies": {
    "@types/babel__generator": "^7.27.0",
    "@types/babel__traverse": "^7.20.7"
  }
}
